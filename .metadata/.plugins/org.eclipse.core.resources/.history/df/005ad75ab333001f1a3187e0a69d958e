package com.ihis.entity;

import java.time.LocalDate;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Entity
@Data
@Table(name = "CITIZEN_GRADUATION_DTLS")
public class CitizenGraduationDtlsEntity {
	
	@Id
	@GeneratedValue
	@Column(name = "GRADUATION_ID")
	private Integer graduation_id;
	
	@Column(name = "CASE_NUM")
	private Integer case_num;
	
	@Column(name = "HIGHEST_DGREE")
	private String highest_dgree;
	
	@Column(name = "GRADUATION_YEAR")
	private String graduation_year;
	
	@Column(name = "UNIVERSITY")
	private String uiniversity;
	
	@Column(name="CREATED_DATE")
	private LocalDate created_date;
	
	@Column(name="UPDATED_DATE")
	private LocalDate updated_date;
	
	@Column(name="CREATED_BY")
	private String Created_by;
	
	@Column(name="UPDATED_BY")
	private String updated_by;

}
